<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Fecha</th>
      <th scope="col">Paciente</th>
      <th scope="col">Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @data.each do |row| %>
      <tr>
        <td scope="row"><%= row.id %></td>
        <td><%= row.date %></td>
        <td><%= row.user.email %></td>
        <td>
          <button class="btn btn-warning">Editar</button>
          <button class="btn btn-danger">Eliminar</button>
          <%# <%= link_to "Editar", in_bodies_edit_path(row.id), class: "btn btn-primary" %>
          <%# <%= link_to "Eliminar", in_bodies_delete_path(row.id), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
